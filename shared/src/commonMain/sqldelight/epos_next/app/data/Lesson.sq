
insert:
INSERT OR IGNORE INTO lessonModel(id, subject, groupId, room, lessonNumber, date, duration, marks)
VALUES (?, ?, ?, ? , ?, ?, ?, ?);

deleteByDate:
DELETE FROM lessonModel
WHERE date BETWEEN date(:from) AND date(:to);

deleteAll:
DELETE FROM lessonModel;

getByDate:
SELECT * FROM lessonModel
WHERE date BETWEEN date(:from) AND date(:to);
